        "rating_process": {
          "steps": [
            {
              "step": 1,
              "name": "Online Registration",
              "description": "Fill EOI Form, pay registration fees, receive login credentials"
            },
            {
              "step": 2,
              "name": "Orientation Workshop",
              "description": "GRIHA Council conducts detailed rating explanation and project-specific guidance"
            },
            {
              "step": 3,
              "name": "Due Diligence I",
              "description": "Site visit during above plinth level construction (column and slab work)"
            },
            {
              "step": 4,
              "name": "Due Diligence II", 
              "description": "Site visit post structural completion to validate finishes, electrical, plumbing, mechanical"
            },
            {
              "step": 5,
              "name": "Document Submission",
              "description": "Upload compliance documents on online panel"
            },
            {
              "step": 6,
              "name": "Preliminary Evaluation",
              "description": "Evaluation by GRIHA Council professionals and external evaluators"
            },
            {
              "step": 7,
              "name": "Final Due Diligence",
              "description": "Final site visit after project completion for verification"
            },
            {
              "step": 8,
              "name": "Final Evaluation",
              "description": "Final rating award based on evaluation and site visit report"
            }
          ],
          "additional_activities": [
            {
              "name": "Green Awareness Drive",
              "description": "Post-rating visit for occupant education on green buildings"
            },
            {
              "name": "Rating Renewal",
              "options": [
                "Submit 3-year audit data report (energy, water, waste)",
                "Enroll for GRIHA EB rating"
